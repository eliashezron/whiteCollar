  "bugs": {
    "url": "https://github.com/eliashezron/professionals/issues"
  },
  "homepage": "https://github.com/eliashezron/professionals#readme",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
  }
}
